pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--monster cycle
--by nandbolt

--actor pools
ghosts={}
zombies={}
skeletons={}
humans={}

--particle system
ps={}

--camera
cx=0
cy=0
caccel=1

--main init
function _init()
	--add player
	make_ghost(hmw,hmh,true)
	
	--add npcs
	make_ghost(hmw-32,hmh,false)
	make_ghost(hmw+32,hmh,false)
	make_ghost(hmw,hmh-32,false)
	make_ghost(hmw,hmh+32,false)
end

--main update
function _update()
	--update particles
	foreach(ps,update_p)
	
	--update actors
	foreach(ghosts,update_ghost)
	
	--update camera
	cx=lerp(cx,player.x-hss,caccel)
	cx=clamp(cx,0,mw-ss)
	cy=lerp(cy,player.y-hss,caccel)
	cy=clamp(cy,0,mh-ss)
	camera(cx,cy)
end

--main draw
function _draw()
	--clear
	cls()
	
	--draw tiles
	map(0,0,0,0,mw,mh)
	
	--draw particles
	foreach(ps,draw_p)
	
	--draw actors
	foreach(ghosts,draw_ghost)
	
	--draw goal
	local xx,yy=cx,cy+1
	cursor(xx,yy)
	print(player.goal,7)
	cursor(xx,yy+8)
	print("hp:"..player.hp.."/"..player.maxhp,7)
	cursor(xx,yy+16)
	print("xp:"..player.xp.."/"..player.maxxp,7)
	cursor(xx,yy+24)
	print("meter:"..player.meter.."/"..player.maxmeter,7)
end
-->8
--math

--constants
mw=128*8 --map size
hmw=128*4 --half map size
mh=128*4 --map size
hmh=128*2 --half map size
ss=128 --screen size
hss=64 --half screen size
ts=8	--tile size
hfs=4 --half tile size

--returns vector2 length
function get_vec_len(x,y)
	return sqrt(x*x+y*y)
end

--lerp
function lerp(a,b,t)
	return a+t*(b-a)
end

--random integer
function irnd(low,high)
	return flr(rnd(high-low+1)+low)
end

--clamp
function clamp(value,low,high)
	if (value<low) return low
	if (value>high) return high
	return value
end

--point collision check
function point_in_box(x,y,x1,y1,x2,y2)
	if (x>=x1 and x<=x2 and
		y>=y1 and y<=y2) then
		return true
	end
	return false
end
-->8
--ghost

--creates a ghost and adds it
--to the list of ghosts
function make_ghost(x,y,is_player)
	local ghost={}
	
	--states
	ghost.dashing=false
	
	--movement
	ghost.x=x
	ghost.y=y
	ghost.vx=0
	ghost.vy=0
	ghost.normspd=2
	ghost.dashspd=4
	ghost.maxspd=ghost.normspd
	ghost.accel=0.1
	ghost.spr=1
	ghost.flipx=false
	ghost.flipy=false
	
	--health
	ghost.maxhp=10
	ghost.hp=ghost.maxhp
	
	--xp
	ghost.maxxp=4
	ghost.xp=0
	
	--meter
	ghost.maxmeter=10
	ghost.meter=ghost.maxmeter
	
	--input
	ghost.dx=0
	ghost.dy=0
	ghost.idash=false
	if is_player then
		ghost.update_input=ghost_player_input
		player=ghost
		player.goal="âœ½fight ghosts."
	else
		ghost.update_input=ghost_npc_input
	end
	
	--add to list
	add(ghosts,ghost)
end

--updates ghost logic
function update_ghost(ghost)
	--get input
	ghost.update_input(ghost)
	
	--update dash
	if ghost.idash then
		ghost.dashing=true
	else
		ghost.dashing=false
	end
	
	--update max speed
	if ghost.dashing then
		ghost.maxspd=ghost.dashspd
	else
		ghost.maxspd=ghost.normspd
	end
	
	--update velocity
	ghost.vx=lerp(ghost.vx,
		ghost.dx*ghost.maxspd,
		ghost.accel)
	ghost.vy=lerp(ghost.vy,
		ghost.dy*ghost.maxspd,
		ghost.accel)
	
	--update position
	ghost.x=clamp(ghost.x+ghost.vx,0,mw-ts)
	ghost.y=clamp(ghost.y+ghost.vy,0,mh-ts)
	
	--update trail
	local ctrail=12
	if (ghost.dashing) ctrail=1
	add_p(ghost.x+irnd(1,6),
		ghost.y+irnd(1,6),ctrail)
	
	--check dash hitbox
	if ghost.dashing then
		for y=ghost.y,ghost.y+ts,ts do
			for x=ghost.x,ghost.x+ts,ts do
				for i=#ghosts,1,-1 do
					local oghost=ghosts[i]
					local x1=oghost.x
					local x2=oghost.x+ts
					local y1=oghost.y
					local y2=oghost.y+ts
					if oghost!=ghost and point_in_box(x,y,x1,y1,x2,y2) then
						ghost_kill(ghost,oghost)
					end
				end
			end
		end
	end
	
	--check gravestone if ready
	if ghost.xp==ghost.maxxp then
		for y=ghost.y,ghost.y+ts,ts do
			for x=ghost.x,ghost.x+ts,ts do
				if mget(x/ts,y/ts)==6 then
					del(ghosts,ghost)
				end
			end
		end
	end
end


--renders the ghost
function draw_ghost(ghost)
	--update sprite
	if abs(ghost.vx)<abs(ghost.vy) then
		--down sprite
		if ghost.dashing then
			ghost.spr=18
		else
			ghost.spr=2
		end
		
		--update y facing direction
		if ghost.vy<0 then
			ghost.flipy=true
		elseif ghost.vy>0 then
			ghost.flipy=false
		end
	else
		--right sprite
		if ghost.dashing then
			ghost.spr=17
		else
			ghost.spr=1
		end
		
		--update x facing direction
		if ghost.vx<0 then
			ghost.flipx=true
		elseif ghost.vx>0 then
			ghost.flipx=false
		end
	end
	
	--ghost sprite
	spr(ghost.spr,ghost.x,ghost.y,
		1,1,ghost.flipx,ghost.flipy)
		
	--draw hitbox
	for y=ghost.y,ghost.y+ts do
		for x=ghost.x,ghost.x+ts do
			if (x==ghost.x or x==ghost.x+ts or
				y==ghost.y or y==ghost.y+ts) then
				pset(x,y,1)
			end
		end
	end
end

--ghost player input
function ghost_player_input(ghost)
	--move input
	ghost.dx=tonum(btn(1))-
		tonum(btn(0))
	ghost.dy=tonum(btn(3))-
		tonum(btn(2))
	
	--action inputs
	ghost.idash=btn(5)
end

--ghost npc input
function ghost_npc_input(ghost)
	ghost.dx=irnd(0,2)-1
	ghost.dy=irnd(0,2)-1
	if rnd(1)>0.5 then
		ghost.idash=true
	else
		ghost.idash=false
	end
end

--ghost kill
function ghost_kill(ghost,oghost)
	--add xp
	ghost.xp=clamp(ghost.xp+1,0,ghost.maxxp)
	
	--destroy other ghost
	del(ghosts,oghost)
end
-->8
--particles

--add particle
function add_p(x,y,c)
	local p={}
	p.x,p.y=x,y
	p.vx,p.vy=0,0
	p.life=10
	p.c=c
	add(ps,p)
end

--update particle
function update_p(p)
	if (p.life<=0) then
		--destroy particle
		del(ps,p)
	else
		--update position
		p.x+=p.vx
		p.y+=p.vy
		
		--update life
		p.life-=1
	end
end

--draw particle
function draw_p(p)
	pset(p.x,p.y,p.c)
end
