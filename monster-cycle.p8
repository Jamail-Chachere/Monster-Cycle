pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--monster cycle
--by nandbolt

--actor pools
ghosts={}
zombies={}
skeletons={}
humans={}

--particle system
ps={}

--camera
cx=0
cy=0
caccel=1

--main init
function _init()
	--add player
	make_ghost(hmw,hmh,true)
	player=ghosts[1]
	
	--add npcs
	make_ghost(hmw-32,hmh,false)
	make_ghost(hmw+32,hmh,false)
	make_ghost(hmw,hmh-32,false)
	make_ghost(hmw,hmh+32,false)
end

--main update
function _update()
	--update particles
	foreach(ps,update_p)
	
	--update actors
	foreach(ghosts,update_ghost)
	
	--update camera
	cx=lerp(cx,player.x-hss,caccel)
	cx=clamp(cx,0,mw-ss)
	cy=lerp(cy,player.y-hss,caccel)
	cy=clamp(cy,0,mh-ss)
	camera(cx,cy)
end

--main draw
function _draw()
	--clear
	cls()
	
	--draw tiles
	--map(flr(cx/8),flr(cy/8),0,0,17,17)
	map(0,0,0,0,mw,mh)
	cursor(cx,cy)
	print(flr(cx/8))
	
	--draw particles
	foreach(ps,draw_p)
	
	--draw actors
	foreach(ghosts,draw_ghost)
end
-->8
--math

--constants
mw=128*8 --map size
hmw=128*4 --half map size
mh=128*4 --map size
hmh=128*2 --half map size
ss=128 --screen size
hss=64 --half screen size
ts=8	--tile size
hfs=4 --half tile size

--returns vector2 length
function get_vec_len(x,y)
	return sqrt(x*x+y*y)
end

--lerp
function lerp(a,b,t)
	return a+t*(b-a)
end

--random integer
function irnd(low,high)
	return flr(rnd(high-low+1)+low)
end

--clamp
function clamp(value,low,high)
	if (value<low) return low
	if (value>high) return high
	return value
end
-->8
--ghost

--creates a ghost and adds it
--to the list of ghosts
function make_ghost(x,y,is_player)
	local ghost={}
	
	--movement
	ghost.x=x
	ghost.y=y
	ghost.vx=0
	ghost.vy=0
	ghost.maxspd=2
	ghost.accel=0.1
	ghost.spr=1
	ghost.flipx=false
	ghost.flipy=false
	
	--input
	ghost.dx=0
	ghost.dy=0
	if is_player then
		ghost.update_input=ghost_player_input
	else
		ghost.update_input=ghost_npc_input
	end
	
	--add to list
	add(ghosts,ghost)
end

--updates ghost logic
function update_ghost(ghost)
	--get input
	ghost.update_input(ghost)
	
	--update velocity
	ghost.vx=lerp(ghost.vx,
		ghost.dx*ghost.maxspd,
		ghost.accel)
	ghost.vy=lerp(ghost.vy,
		ghost.dy*ghost.maxspd,
		ghost.accel)
	
	--update position
	ghost.x=clamp(ghost.x+ghost.vx,0,mw-ts)
	ghost.y=clamp(ghost.y+ghost.vy,0,mh-ts)
	
	--update trail
	add_p(ghost.x+irnd(1,6),
		ghost.y+irnd(1,6))
end


--renders the ghost
function draw_ghost(ghost)
	--update sprite
	if abs(ghost.vx)<abs(ghost.vy) then
		ghost.spr=2
		
		--update y facing direction
		if ghost.vy<0 then
			ghost.flipy=true
		elseif ghost.vy>0 then
			ghost.flipy=false
		end
	else
		ghost.spr=1
		
		--update x facing direction
		if ghost.vx<0 then
			ghost.flipx=true
		elseif ghost.vx>0 then
			ghost.flipx=false
		end
	end
	
	--ghost sprite
	spr(ghost.spr,ghost.x,ghost.y,
		1,1,ghost.flipx,ghost.flipy)
end

--ghost player input
function ghost_player_input(ghost)
	ghost.dx=tonum(btn(1))-
		tonum(btn(0))
	ghost.dy=tonum(btn(3))-
		tonum(btn(2))
end

--ghost npc input
function ghost_npc_input(ghost)
	ghost.dx=irnd(0,2)-1
	ghost.dy=irnd(0,2)-1
end
-->8
--particles

--add particle
function add_p(x,y)
	local p={}
	p.x,p.y=x,y
	p.vx,p.vy=0,0
	p.life=10
	add(ps,p)
end

--update particle
function update_p(p)
	if (p.life<=0) then
		--destroy particle
		del(ps,p)
	else
		--update position
		p.x+=p.vx
		p.y+=p.vy
		
		--update life
		p.life-=1
	end
end

--draw particle
function draw_p(p)
	pset(p.x,p.y,12)
end
